{
  "name": "vuetify-project",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "main": "electron.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron:dev": "vite & electron .",
    "electron:build": "vite build && electron-builder",
    "preview": "vite preview",
    "lint": "eslint . --fix"
  },
  "build": {
    "appId": "com.example.vuetify-project",
    "productName": "Vuetify Project",
    "files": [
      "dist/**/*",
      "electron.js"
    ],
    "directories": {
      "output": "dist_electron"
    },
    "win": {
      "target": "nsis"
    }
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.7",
    "@mdi/font": "7.4.47",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vuetify": "^3.10.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.5",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.35.0",
    "eslint-config-vuetify": "^4.2.0",
    "sass-embedded": "^1.92.1",
    "unplugin-fonts": "^1.4.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.3.1",
    "vite-plugin-vuetify": "^2.1.2"
  }
